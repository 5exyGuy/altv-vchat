{
    "type": "object",
    "properties": {
        "emojis": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "The name of the emoji, e.g. 'thumbsup' or 'smile'"
                    },
                    "textEquivalent": {
                        "type": "string",
                        "description": "The text equivalent of the emoji, e.g. '+' or ':)'"
                    },
                    "extension": {
                        "type": "string",
                        "enum": ["gif", "jpg", "png", "svg", "webp", "apng"],
                        "description": "The file extension of the emoji, e.g. 'png' or 'gif'"
                    },
                    "url": {
                        "type": "string",
                        "description": "The URL of the emoji, e.g. 'https://example.com/emoji.png'"
                    }
                }
            }
        }
    }
}
